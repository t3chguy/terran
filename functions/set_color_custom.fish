function set_color_custom
    set -U fish_color_normal                normal
    set -U fish_color_command               99f
    set -U fish_color_param                 fff
    set -U fish_color_redirection           fff
    set -U fish_color_comment               555
    set -U fish_color_error                 fc0
    set -U fish_color_escape                fc0
    set -U fish_color_operator              fc0
    set -U fish_color_end                   33f
    set -U fish_color_quote                 33f
    set -U fish_color_autosuggestion        555
    set -U fish_color_valid_path            --underline
    set -U fish_color_cwd                   99f
    set -U fish_color_cwd_root              99f
    set -U fish_color_match                 99f
    set -U fish_color_search_match          --background=33f
    set -U fish_color_selection             --background=33f
    set -U fish_pager_color_prefix          white
    set -U fish_pager_color_completion      white -o
    set -U fish_pager_color_description     white
    set -U fish_pager_color_progress        fc0
    set -U fish_color_history_current       fc0
end
